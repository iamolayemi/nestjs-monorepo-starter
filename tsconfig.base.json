{
    "compilerOptions": {
        "target": "es2017",
        "module": "commonjs",
        "esModuleInterop": true,
        "declaration": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "lib": ["es2017", "dom"],
        "moduleResolution": "node",
        "skipDefaultLibCheck": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true
    }
}
